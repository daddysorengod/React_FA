{
  "name": "fund-admin-frontend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "version": "1.0.0",
    "dev": "next dev",
    "build": "rm -r -f .next && next build",
    "build:dev": "npm run env:dev && rm -r -f .next && next build && rm -r -f .next.zip && zip -r fund-admin-web-frontend-dev.zip .next public && npm run deliver:build_dev",
    "build:uat": "npm run env:uat && rm -r -f .next && next build && rm -r -f .next.zip && zip -r fund-admin-web-frontend-uat.zip .next public && npm run deliver:build_uat",
    "build:pre_prod": "npm run env:pre_prod && rm -r -f .next && next build && rm -r -f .next.zip && zip -r fund-admin-web-frontend-pre-production.zip .next public && npm run deliver:build_pre_prod",
    "build:prod": "npm run env:prod && rm -r -f .next && next build && rm -r -f .next.zip && zip -r fund-admin-web-frontend-production.zip .next public && npm run deliver:build_prod",
    "prod:zip":"zip -r fund-admin-web-frontend.zip .next public",
    "start": "next start -p 8000",
    "start:custom": "NODE_ENV=production node server.js",
    "lint": "next lint",
    "type-check": "tsc",
    "lint:fix": "./node_modules/.bin/tslint --fix -c ./tslint.json -p ./tsconfig.json",
    "env:dev": "cp -r ./env/env.development ./.env",
    "env:uat": "cp -r ./env/env.uat ./.env",
    "env:pre_prod": "cp -r ./env/env.preproduction ./.env",
    "env:prod": "cp -r ./env/env.production ./.env",
    "deliver:build_dev":"rm -r -f ./build/fund-admin-web-frontend-dev.zip && cp -r ./fund-admin-web-frontend-dev.zip ./build/fund-admin-web-frontend-dev.zip && rm -r -f fund-admin-web-frontend-dev.zip",
    "deliver:build_uat":"rm -r -f ./build/fund-admin-web-frontend-uat.zip && cp -r ./fund-admin-web-frontend-uat.zip ./build/fund-admin-web-frontend-uat.zip && rm -r -f fund-admin-web-frontend-uat.zip",
    "deliver:build_pre_prod":"rm -r -f ./build/fund-admin-web-frontend-pre-production.zip && cp -r ./fund-admin-web-frontend-pre-production.zip ./build/fund-admin-web-frontend-pre-production.zip && rm -r -f fund-admin-web-frontend-pre-production.zip",
    "deliver:build_prod":"rm -r -f ./build/fund-admin-web-frontend-production.zip && cp -r ./fund-admin-web-frontend-production.zip ./build/fund-admin-web-frontend-production.zip && rm -r -f fund-admin-web-frontend-production.zip"
  },
  "proxy": "http://localhost:3090",
  "dependencies": {
    "@ant-design/colors": "^6.0.0",
    "@ant-design/icons": "^4.7.0",
    "@emotion/cache": "^11.10.5",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@material-ui/core": "^4.12.4",
    "@material-ui/styles": "^4.11.5",
    "@monaco-editor/react": "^4.5.1",
    "@mui/icons-material": "^5.11.16",
    "@mui/lab": "^5.0.0-alpha.103",
    "@mui/material": "^5.10.9",
    "@mui/styles": "^5.12.0",
    "@mui/x-date-pickers": "^6.2.1",
    "@reduxjs/toolkit": "^1.9.5",
    "@types/node": "18.16.2",
    "@types/react": "18.2.0",
    "@types/react-dom": "18.2.1",
    "axios": "^1.4.0",
    "dayjs": "^1.11.7",
    "dotenv": "^16.3.1",
    "eslint": "8.39.0",
    "eslint-config-next": "13.3.1",
    "export-to-csv": "^0.2.1",
    "framer-motion": "^10.12.4",
    "jquery": "^3.6.4",
    "jquery-validation": "^1.19.5",
    "js-cookie": "^3.0.5",
    "lodash": "^4.17.21",
    "material-react-table": "^1.11.0",
    "material-ui-popup-state": "^5.0.8",
    "moment": "^2.29.4",
    "monaco-editor": "^0.39.0",
    "next": "13.3.1",
    "next-auth": "^4.19.2",
    "next-translate": "^2.0.5",
    "notistack": "^2.0.8",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-draggable": "^4.4.5",
    "react-dropzone": "^14.2.3",
    "react-imask": "^6.6.1",
    "react-json-view": "^1.21.3",
    "react-material-ui-carousel": "^3.4.2",
    "react-redux": "^8.0.5",
    "redux": "^4.2.1",
    "redux-persist": "^6.0.0",
    "stylis-plugin-rtl": "^2.1.1",
    "typescript": "5.0.4"
  },
  "devDependencies": {
    "@builder.io/partytown": "^0.8.0",
    "@types/jquery": "^3.5.16",
    "@types/jquery.validation": "^1.16.7"
  }
}
